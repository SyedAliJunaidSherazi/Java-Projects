<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.SignInController">
   <children>
      <Label layoutX="177.0" layoutY="41.0" prefHeight="30.0" prefWidth="261.0" text="Provide Details To Register">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <AnchorPane layoutX="92.0" layoutY="71.0" prefHeight="209.0" prefWidth="415.0">
         <children>
            <Label layoutX="23.0" layoutY="20.0" prefHeight="19.0" prefWidth="57.0" text="Name">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label layoutX="22.0" layoutY="54.0" prefHeight="19.0" prefWidth="68.0" text="Password">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label layoutX="22.0" layoutY="95.0" prefHeight="19.0" prefWidth="57.0" text="Address">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label layoutX="23.0" layoutY="136.0" prefHeight="19.0" prefWidth="68.0" text="Phone No">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Label layoutX="22.0" layoutY="177.0" prefHeight="17.0" prefWidth="40.0" text="CNIC">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="userName" layoutX="133.0" layoutY="17.0" />
            <TextField fx:id="userPassword" layoutX="133.0" layoutY="51.0" />
            <TextField fx:id="userAddress" layoutX="133.0" layoutY="92.0" />
            <TextField fx:id="userPhNo" layoutX="133.0" layoutY="133.0" />
            <TextField fx:id="userCNIC" layoutX="133.0" layoutY="173.0" />
         </children>
      </AnchorPane>
      <Button fx:id="register" layoutX="252.0" layoutY="316.0" mnemonicParsing="false" onAction="#pressRegister" text="Register" textFill="#087596">
         <font>
            <Font name="Bell MT Italic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="guest" layoutX="383.0" layoutY="316.0" mnemonicParsing="false" onAction="#guestController" text="Enter as Guest" textAlignment="CENTER" textFill="#087596">
         <font>
            <Font name="Bell MT Italic" size="16.0" />
         </font>
      </Button>
      <Button layoutX="133.0" layoutY="316.0" mnemonicParsing="false" onAction="#back" text="Back" textFill="#087596">
         <font>
            <Font name="Bell MT Italic" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>

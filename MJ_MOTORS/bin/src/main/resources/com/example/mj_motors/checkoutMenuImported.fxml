<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.GuestController">
    <children>
        <Label layoutX="106.0" layoutY="53.0" text="Imported Cars Checkout Section" textFill="#69065f">
            <font>
                <Font name="Book Antiqua Bold" size="27.0" />
            </font>
        </Label>
        <Button fx:id="newCarSec" layoutX="64.0" layoutY="189.0" mnemonicParsing="false" onAction="#newCarsSectionGuestImp" text="New Cars Section" textFill="#056d76">
            <font>
                <Font name="Book Antiqua Bold Italic" size="20.0" />
            </font>
        </Button>
        <Button fx:id="OldCarSec" layoutX="369.0" layoutY="189.0" mnemonicParsing="false" onAction="#oldCarsSectionGuestImp" text="Old Cars Section" textFill="#056d76">
            <font>
                <Font name="Book Antiqua Bold Italic" size="20.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="221.0" layoutY="319.0" mnemonicParsing="false" onAction="#backToGuestImp" text="Back To Guest Menu" textFill="#a40101">
            <font>
                <Font name="Book Antiqua" size="17.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

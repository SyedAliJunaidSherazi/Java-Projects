<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.GuestController">
    <children>
        <Label layoutX="128.0" layoutY="29.0" text="Local Cars Section" textFill="#06808c">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Label>
        <Button fx:id="purchaseLocal" layoutX="98.0" layoutY="185.0" mnemonicParsing="false" onAction="#checkoutLocalCars" text="Check Out Available Local Cars">
            <font>
                <Font name="Engravers MT" size="16.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="260.0" layoutY="337.0" mnemonicParsing="false" onAction="#backToGuest" text="Back">
            <font>
                <Font name="Engravers MT" size="16.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

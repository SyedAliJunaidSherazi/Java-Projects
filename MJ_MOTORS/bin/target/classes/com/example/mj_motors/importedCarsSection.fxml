<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.ImportedCarsController">
    <children>
        <Label layoutX="98.0" layoutY="32.0" text="IMPORTED Cars Section" textFill="#06808c">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Label>
        <Button fx:id="purchaseLocal" layoutX="141.0" layoutY="115.0" mnemonicParsing="false" onAction="#purchaseImportedCars" text="Purchase IMPORTED Cars">
            <font>
                <Font name="Engravers MT" size="16.0" />
            </font>
        </Button>
        <Button fx:id="saleLocal" layoutX="172.0" layoutY="200.0" mnemonicParsing="false" onAction="#saleImportedCarsController" text="Sale IMPORTEDCars">
            <font>
                <Font name="Engravers MT" size="16.0" />
            </font>
        </Button>
        <Button fx:id="commissionLocal" layoutX="174.0" layoutY="284.0" mnemonicParsing="false" onAction="#placeCarForSaleController" text="Place Car For Sale">
            <font>
                <Font name="Engravers MT" size="16.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="501.0" layoutY="355.0" mnemonicParsing="false" onAction="#back" text="Back">
            <font>
                <Font name="Engravers MT" size="16.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
